#ifndef RCC_INTERFACE_H_INCLUDDED
#define RCC_INTERFACE_H_INCLUDDED

typedef enum
{
	RCC_ADC
}PeripheralID_t;

void MRCC_voidInitClock(void);

void MRCC_voidEnablePeripheral(PeripheralID_t);

void MRCC_voidDisablePeripheral(PeripheralID_t);



